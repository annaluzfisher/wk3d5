class MyStack

    def initialize
        @store = []
        @size = 0

    end

    def push(ele)
            @store.unshift(ele)
            @size += 1
    end

    def pop(ele)
        @store.shift(ele)

        @size -= 1 unless @size == 0
    end

    def peek
        @store.first
    end

    def inspect
        "haha got you"
        
    end
end

